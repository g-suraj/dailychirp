---
import Header from "../components/Header.astro";
import MainGame from "../components/MainGame.astro";
import "../styles/global.css";

const data = await fetch(
  "https://suraj.fyi/xeno-canto-processed/pickled_data.json",
);
const birds = await data.json();
const birds_str = JSON.stringify(birds);
---

<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="icon" type="image/svg+xml" href="favicon.ico" />
    <meta name="generator" content={Astro.generator} />
    <title>Daily Chirp</title>
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/bulma@0.9.4/css/bulma.min.css"
    />
  </head>
  <body>
    <Header />
    <MainGame birds={birds_str} />
  </body>
</html>
